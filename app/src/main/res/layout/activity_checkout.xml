<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:forceDarkAllowed="false"
    android:layout_height="match_parent">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:text="@string/checkout_string"
                android:textColor="@color/black"
                android:textSize="30sp"
                android:textStyle="bold" />

            <RelativeLayout
                android:layout_marginTop="240dp"
                android:layout_marginStart="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp">

                <TextView
                    android:alpha="0.5"
                    android:textSize="12sp"
                    android:textColor="@color/checkout_color"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="0dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/page_title_string"
                    />

                <TextView
                    android:textStyle="bold"
                    android:textSize="15sp"
                    android:textColor="@color/black"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/on_delivery"
                    />
                <ImageView android:id="@+id/ic_edit_24px_ek1"
                    android:src="@drawable/ic_baseline_edit_24"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_marginEnd="15dp" />


                <ImageView
                    android:src="@drawable/line_16"
                    android:contentDescription="line_16"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="63dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="0dp" />

            </RelativeLayout>


            <RelativeLayout
                android:layout_marginTop="70dp"
                android:layout_marginStart="11dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="11dp">

                <ImageView
                    android:id="@+id/addressEdit"
                    android:src="@drawable/ic_baseline_edit_24"
                    android:contentDescription="ic_edit_24px_ek2"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="20dp"
                    android:layout_marginRight="15dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="315.99951171875dp" />


                <TextView
                    android:alpha="0.5"
                    android:textSize="12sp"
                    android:textColor="@color/checkout_color"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="0dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/page_title_ek1_string"
                    />

                <TextView android:id="@+id/shaker_shaker_no_123__sub_street___kharian__gurjat__punjab__pakistan"
                    android:textStyle="bold"
                    android:textSize="15sp"
                    android:textColor="@color/checkout_color"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="20dp"
                    android:layout_width="131dp"
                    android:layout_height="wrap_content"
                    />

                <EditText android:id="@+id/etAddress"
                    android:textStyle="bold"
                    android:textSize="15sp"
                    android:textColor="@color/checkout_color"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="20dp"
                    android:layout_width="131dp"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@id/line"
                    android:layout_marginBottom="2dp"
                    android:text="Please Check And Confirm Your Address" />

                <ImageView
                    android:id="@+id/line"
                    android:src="@drawable/line_4"
                    android:contentDescription="line_4"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="151dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="0dp" />

            </RelativeLayout>

            <TextView
                android:textSize="15sp"
                android:textColor="@color/add_promo_code_color"
                android:layout_marginLeft="52dp"
                android:layout_marginTop="342dp"
                android:layout_width="wrap_content"
                android:layout_height="30.5dp"
                android:text="@string/add_promo_code_string"
                />


            <ImageView
                android:src="@drawable/ic_baseline_edit_24"
                android:contentDescription="ic_edit_24px_ek2"
                android:layout_alignParentRight="true"
                android:layout_marginRight="15dp"
                android:layout_marginTop="341.99755859375dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="315.99951171875dp" />
    <androidx.cardview.widget.CardView
        android:id="@+id/backBtnCheckout"
        android:layout_margin="4dp"
        android:layout_width="40dp"
        android:layout_height="40dp">
        <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@color/app"/>

        <ImageView
            android:layout_width="30dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="11dp"
            android:layout_marginTop="5dp"
            android:contentDescription="icon_ionic_ios_arrow_back"
            android:src="@drawable/ic_baseline_arrow_back_ios_24" />
    </androidx.cardview.widget.CardView>



            <ImageView
                android:src="@drawable/ic_baseline_local_offer_24"
                android:contentDescription="path_16"
                android:layout_marginStart="16dp"
                android:layout_marginTop="341.99755859375dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp" />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/color_assets_5"
        android:layout_alignParentBottom="true">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Total"
            android:layout_marginLeft="40sp"
            android:textSize="20sp"/>
        <TextView
            android:id="@+id/totalBillCheckout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="10dp"
            android:textSize="35sp"
            android:textStyle="bold"
            android:layout_marginBottom="10dp"
            android:textColor="@color/black"
            android:text="Rs.1245"
            />

        <com.ebanx.swipebtn.SwipeButton
            android:id="@+id/PlaceOrderBtn"
            android:layout_width="15dp"
            android:layout_height="40dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginStart="90dp"
            android:layout_marginLeft="90dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="20dp"
            android:layout_marginRight="40dp"
            android:layout_toEndOf="@+id/totalBillCheckout"
            android:layout_toRightOf="@+id/totalBillCheckout"
            app:button_background="@drawable/textboxround"
            app:button_image_disabled="@drawable/ic_baseline_arrow_forward_ios_appcolor"
            app:button_image_enabled="@drawable/ic_baseline_arrow_forward_ios_appcolor"
            app:button_left_padding="2dp"
            app:button_right_padding="2dp"
            app:button_top_padding="3dp"
            app:button_bottom_padding="3dp"
            app:has_activate_state="true"
            app:initial_state="disabled"
            app:inner_text="Place Order"
            app:inner_text_background="@drawable/square"
            app:inner_text_bottom_padding="10dp"
            app:inner_text_color="@color/white"
            app:inner_text_size="8dp"
            app:inner_text_left_padding="20dp"
            app:inner_text_top_padding="10dp"
            android:layout_alignParentEnd="true" />
    </RelativeLayout>


</RelativeLayout>